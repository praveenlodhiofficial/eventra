{
   "semi": true,
   "tabWidth": 3,
   "printWidth": 100,
   "singleQuote": false,
   "trailingComma": "es5",
   "bracketSpacing": true,
   "arrowParens": "always",
   "endOfLine": "lf",
   "useTabs": false,
   "quoteProps": "as-needed",
   "jsxSingleQuote": false,
   "plugins": ["prettier-plugin-tailwindcss"]
}
